{
  "manifest_version": 3,
  "name": "SproutGigs Payment Countdown",
  "version": "1.0",
  "description": "Made by polando8 for SproutGigs's Gigs payment credit due date countdown.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://sproutgigs.com/wallet.php*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
